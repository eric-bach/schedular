{
  "name": "schedular",
  "version": "0.1.0",
  "repository": "https://github.com/eric-bach/schedular",
  "bin": {
    "schedular": "bin/schedular.js"
  },
  "scripts": {
    "build": "tsc",
    "build-all": "npm run cdk-clear-context && ts-node ./helpers/commands.ts \"npm run build\" \"node_modules, cdk.out, build\"",
    "cdk": "cdk",
    "cdk-clear-context": "npm run cdk -- context --clear",
    "clean-all": "npm run cdk-clear-context && ts-node ./helpers/commands.ts \"rm -fR node_modules\" \"node_modules, cdk.out, build\"",
    "deploy": "npm run build && npm run build-all && ts-node ./helpers/deploy.ts",
    "deploy-backend": "npm run quick-deploy -- -s backend",
    "deploy-frontend": "npm run quick-deploy -- -s frontend",
    "install-all": "npm i && npm run cdk-clear-context && ts-node ./helpers/commands.ts \"npm install\" \"node_modules, cdk.out, build\"",
    "quick-deploy": "ts-node ./helpers/deploy.ts",
    "seed": "npm run build && ts-node ./helpers/seed.ts",
    "test": "npm run build && jest --watchAll=false --coverage",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@aws-appsync/eslint-plugin": "^1.1.0",
    "@aws-appsync/utils": "^1.1.0",
    "@aws-sdk/client-appsync": "^3.309.0",
    "@aws-sdk/client-cloudformation": "^3.287.0",
    "@aws-sdk/client-dynamodb": "^3.287.0",
    "@aws-sdk/client-ses": "^3.300.0",
    "@aws-sdk/credential-providers": "^3.293.0",
    "@aws-sdk/util-dynamodb": "^3.287.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.2",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "aws-cdk": "^2.74.0",
    "aws-cdk-lib": "^2.74.0",
    "commander": "^10.0.0",
    "constructs": "^10.0.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "fs": "^0.0.1-security",
    "jest": "^29.4.3",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5",
    "uuid": "^9.0.0"
  }
}
